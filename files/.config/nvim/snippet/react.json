{
    "react-fragment":      { "prefix": "fragment",      "body": [ "<React.Fragment>\n\t${1:${TM_SELECTED_TEXT}}\n</React.Fragment>"               ] },
    "react-class":         { "prefix": "class-name",    "body": [ "className=\"${1}\""                                                            ] },
