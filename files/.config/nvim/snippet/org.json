{
    "author": {
        "prefix": "author",
        "description": "author",
        "body": [
            "#+author: $0"
        ]
    },
    "center": {
        "prefix": "center",
        "description": "center",
        "body": [
            "#+begin_center",
            "$0",
            "#+end_center"
        ]
    },
    "date": {
        "prefix": "date",
        "description": "date",
        "body": [
            "#+date: ${1:year}:${2:month}:${3:day}"
        ]
    },
    "description": {
        "prefix": "desc",
        "description": "desc",
        "body": [
            "#+description: $0"
        ]
    },
    "dot": {
        "prefix": "dot_",
        "description": "dot_",
        "body": [
            "#+begin_src dot :file ${1:file} :cmdline -t${2:pdf} :exports none :results silent",
            "$0",
            "#+end_src",
            "[[file:${3:path}]]"
        ]
    },
    "elisp": {
        "prefix": "elisp_",
        "description": "elisp_",
        "body": [
            "#+begin_src emacs-lisp :tangle yes",
            "$0",
            "#+end_src"
        ]
    },
    "emacs-lisp": {
        "prefix": "emacs-lisp_",
        "description": "emacs-lisp_",
        "body": [
            "#+begin_src emacs-lisp :tangle yes",
            "$0",
            "#+end_src"
        ]
    },
    "email": {
        "prefix": "email",
        "description": "email",
        "body": [
            "#+email: $0"
        ]
    },
    "embedded": {
        "prefix": "emb_",
        "description": "emb_",
        "body": [
            "src_${1:lang}${2:[${3:where}]}{${4:code}}"
        ]
    },
    "entry": {
        "prefix": "entry_",
        "description": "entry_",
        "body": [
            "#+begin_html",
            "layout: ${1:default}",
            "title: ${2:title}",
            "#+end_html"
        ]
    },
    "example": {
        "prefix": "example",
        "description": "example",
        "body": [
            "#+begin_example",
            "$0",
            "#+end_example"
        ]
    },
    "export": {
        "prefix": "export",
        "description": "export",
        "body": [
            "#+begin_export ${1:type}",
            "$0",
            "#+end_export"
        ]
    },
    "figure": {
        "prefix": "fig_",
        "description": "fig_",
        "body": [
            "#+caption: ${1:caption}",
            "#+attr_latex: ${2:scale=0.75}",
            "#+label: fig:${3:label}$0"
        ]
    },
    "html": {
        "prefix": "html",
        "description": "htmlt",
        "body": [
            "#+html:$1"
        ]
    },
    "image": {
        "prefix": "image",
        "description": "imagem",
        "body": [
            "#+caption: ${1:caption of the image}",
            "[[file:${2:image_path}]]$0"
        ]
    },
    "img": {
        "prefix": "img_",
        "description": "img_",
        "body": [
            "<img src=\"$1\" alt=\"$2\" align=\"${3:left}\" title=\"${4:image title}\" class=\"img\" $5/>$0"
        ]
    },
    "include": {
        "prefix": "include",
        "description": "include",
        "body": [
            "#+include: $0"
        ]
    },
    "ipython": {
        "prefix": "ipy_",
        "description": "ipy_",
        "body": [
            "#+begin_src ipython :session ${1:session01} :file ${2:$.png)} :exports ${3:both}",
            "$0",
            "#+end_src"
        ]
    },
    "keywords": {
        "prefix": "keywords",
        "description": "keywordse",
        "body": [
            "#+keywords: $0"
        ]
    },
    "language": {
        "prefix": "language",
        "description": "languagean",
        "body": [
            "#+language: ${1:en}"
        ]
    },
    "link": {
        "prefix": "link",
        "description": "linki",
        "body": [
            "[[${1:link}][${2:description}]]"
        ]
    },
    "options": {
        "prefix": "options",
        "description": "optionsp",
        "body": [
            "#+options: h:${1:1} num:${2:t||nil} toc:${3:t||nil}$0"
        ]
    },
    "python": {
        "prefix": "py_",
        "description": "py_",
        "body": [
            "#+begin_src python",
            "$0",
            "#+end_src"
        ]
    },
    "quote": {
        "prefix": "quote",
        "description": "quote",
        "body": [
            "#+begin_quote",
            "$0",
            "#+end_quote"
        ]
    },
    "reveal_single_colored_background": {
        "prefix": "reveal_single_colored_background",
        "description": "reveal_single_colored_backgroundsb",
        "body": [
            ":properties:",
            ":reveal_background: ${1: #123456}",
            ":end:"
        ]
    },
    "reveal_image_background": {
        "prefix": "reveal_image_background",
        "description": "reveal_image_backgroundib",
        "body": [
                ":properties:",
                ":reveal_background: ${1: path of the image}",
                ":reveal_background_trans: ${2: default||cube||page||concave||zoom||linear||fade||none||slide}",
                ":end:"
            ]
        },
    "setup": {
        "prefix": "set",
        "description": "set",
        "body": [
            "#+setupfile: $0"
        ]
    },
    "src": {
        "prefix": "src",
        "description": "srcrc",
        "body": [
            "#+begin_src $1",
              "$0",
            "#+end_src"
        ]
    },
    "style": {
        "prefix": "style",
        "description": "stylet",
        "body": [
            "#+style: <link rel=\"stylesheet\" type=\"text/css\" href=\"$1\" />"
        ]
    },
    "table": {
        "prefix": "table",
        "description": "tablea",
        "body": [
            "#+caption: ${1: caption of the table}",
            "|${2:column 1} | ${3: column 2} |",
            "|--------------+----------------|"
        ]
    },
    "title": {
        "prefix": "title",
        "description": "title",
        "body": [
            "#+title: $0"
        ]
    },
    "uml": {
        "prefix": "uml",
        "description": "uml",
        "body": [
            "#+begin_uml",
            "$1",
            "#+end_uml"
        ]
    },
    "verse": {
        "prefix": "verse",
        "description": "verse",
        "body": [
            "#+begin_verse",
            "$0",
            "#+end_verse"
        ]
    },
    "video": {
        "prefix": "video",
        "description": "video",
        "body": [
            "[[${1:link of the video}][file:${2:link of the image}]"
        ]
    }
}
