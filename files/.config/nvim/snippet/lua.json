{
    "fun": {
        "prefix": "fun",
        "description": "fun",
        "body": [
        "function${1}(${2})",
        "    ${0}",
        "end"
        ]
    },
    "for": {
        "prefix": "for",
        "description": "for",
        "body": [
        "for ${1} do",
        "    ${0}",
        "end"
        ]
    },
    "forp": {
        "prefix": "for",
        "description": "for key,value pairs",
        "body": [
        "for ${1:i},${2:v} in pairs(${3:tbl}) do",
        "    ${0}",
        "end"
        ]
    },
    "fori": {
        "prefix": "fori",
        "description": "indexi, value pairs",
        "body": [
        "for ${1:i},${2:v} in ipairs(${3:tbl}) do",
        "    ${0}",
        "end"
        ]
    },
    "forn": {
        "prefix": "forn",
        "description": "for",
        "body": [
        "for ${1:i}=${2:1},${3:10} do",
        "    ${0}",
        "end"
        ]
    },
    "if": {
        "prefix": "if",
        "description": "if",
        "body": [
        "if ${1:true} then",
        "    ${0}",
        "end"
        ]
    },
    "elif": {
        "prefix": "elif",
        "description": "elif",
        "body": [
        "elseif ${1:condition} then",
        "    ${0:--body}"
        ]
    },
    "repeat": {
        "prefix": "repeat",
        "description": "repeat",
        "body": [
        "repeat",
        "    ${0}",
        "until ${1:true}"
        ]
    },
    "while": {
        "prefix": "while",
        "description": "while",
        "body": [
        "while ${1:condition} do",
        "    ${0}",
        "end"
        ]
    },
    "print": {
        "prefix": "print",
        "description": "print",
        "body": [
        "print(${0:${TM_SELECTED_TEXT}})"
        ]
    },
    "import": {
        "prefix": "import",
        "description": "import",
        "body": [
        "import \"${1:import file}\""        ]
    }
}
