{
    "image": {
        "prefix": "image",
        "description": "img ![...](...)",
        "body": [
            "![${1:text}](${2:http://address})"
        ]
    },
    "link": {
        "prefix": "link",
        "description": "link [...](...)",
        "body": [
            "[${1:alttext}](${2:/images/image.jpg})"
        ]
    },
    "__": {
        "prefix": "__",
        "description": "__bold__",
        "body": [
            "__${1:bold}__"
        ]
    },
    "item-": {
        "prefix": "i-",
        "description": "-",
        "body": [
            "-   ${0}"
        ]
    },
    "item*": {
        "prefix": "i*",
        "description": "*",
        "body": [
            "*   ${0}"
        ]
    },
    "item+": {
        "prefix": "i+",
        "description": "+",
        "body": [
            "+   ${0}"
        ]
    },
    "code": {
        "prefix": "code",
        "description": "```vim",
        "body": [
            "```${1:vim}",
            "   ${0}",
            "```"
        ]
    },
    "table": {
        "prefix": "table",
        "description": "table |---|",
        "body": [
            "| fact | a | b |",
            "| ---- | - | - |",
            "| f1   | Y | N |",
            "| f2   | Y | N |"
        ]
    }
}
