{
  "template": {
    "prefix": "template",
    "body": [
      "---",
      "apiVersion: apps/v1",
      "kind: Deployment",
      "metadata:",
      "  name: nginx-example",
      "  labels:",
      "    app: nginx",
      "spec:",
      "  replicas: 1",
      "  selector:",
      "    matchLabels:",
      "      app: nginx",
      "  template:",
      "    metadata:",
      "      labels:",
      "        app: nginx",
      "    spec:",
      "      containers:",
      "        - name: nginx",
      "          image: nginx:1.21-alpine",
      "          ports:",
      "          - containerPort: 80"
    ]
  }
}
