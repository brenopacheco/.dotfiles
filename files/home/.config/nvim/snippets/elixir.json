{
  "defmodule": {
    "prefix": ["defmodule"],
    "body": [
      "defmodule ${1:Module} do",
      "\tdef ${2:function} do",
      "\t\t${0}",
      "\tend",
      "end"
    ]
  },
  "deffun": {
    "prefix": ["def", "deffun", "defun"],
    "body": [
      "def ${function} do",
      "\t${0}",
      "end"
    ]
  }
}
