Host github.com
	HostName github.com
	User git

Host github-blp.com
	HostName github.com
	User git
	IdentityFile ~/.ssh/blp.pub # gpg --export-ssh-key <email>
	IdentitiesOnly yes
	# < ~/.gitconfig
	# [url "git@github-blp.com:brenopacheco/"]
	#     insteadOf = git@github.com:brenopacheco/
	#     insteadOf = https://github.com/brenopacheco/

